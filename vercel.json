{
    "framework": "vite",
    "buildCommand": "vite build",
    "outputDirectory": "dist",
    "functions": {
        "api/**/*.js": {
            "runtime": "nodejs18.x"
        }
    },
    "rewrites": [
        {
            "source": "/api/(.*)",
            "destination": "/api/$1"
        }
    ]
}